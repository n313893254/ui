<section>
  {{metrics-action 
    resourceType="pod"
    id=pod.name
    namespaceId=pod.namespace.id
    dashboards=dashboards
    dashboardName="Pods"
    queryAction="query"
    allowHide=true
    state=state
  }}
  {{#unless state.hide}}
    {{metrics-graph graphs=graphs loading=state.loading noGraphs=state.noGraphs}}
    <hr class="mt-30"/>
  {{/unless}}
</section>